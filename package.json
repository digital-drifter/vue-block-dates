{
  "name": "vue-hotel-datepicker",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.ts",
  "typeScriptVersion": "2.9.2",
  "files": [
    "dist/*.js",
    "types/*.d.ts"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "rimraf dist && vue-cli-service build --mode production",
    "build:dist": "rimraf dist && cross-env NODE_ENV=production webpack --config build/config.js --progress --hide-modules",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "json-server": "node_modules/.bin/json-server --watch server/db.json --routes server/routes.json -c server/json-server.json",
    "bundle": "yarn bundle:esm && yarn bundle:cjs && yarn bundle:umd && yarn bundle:umd-production",
    "bundle:esm": "rollup -c rollup/esm.js",
    "bundle:cjs": "rollup -c rollup/cjs.js",
    "bundle:umd": "rollup -c rollup/umd.dev.js",
    "bundle:umd-production": "rollup -c rollup/umd.production.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.2",
    "@fortawesome/free-solid-svg-icons": "^5.2.0",
    "@fortawesome/vue-fontawesome": "^0.1.1",
    "bootstrap": "4.1.3",
    "dayjs": "1.7.4",
    "v-tooltip": "^2.0.0-rc.33",
    "vuejs-datepicker": "^1.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.55",
    "@fortawesome/fontawesome-common-types": "^0.2.2",
    "@types/jest": "^23.1.4",
    "@vue/cli-plugin-babel": "^3.0.0-rc.9",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.9",
    "@vue/cli-plugin-unit-jest": "^3.0.0-rc.9",
    "@vue/cli-service": "^3.0.0-rc.9",
    "@vue/test-utils": "^1.0.0-beta.22",
    "babel-loader": "^8.0.0-beta.4",
    "compression-webpack-plugin": "^1.1.11",
    "dotenv": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "happypack": "^5.0.0",
    "jest": "^23.4.2",
    "jest-cli": "^23.4.2",
    "json-server": "^0.14.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.4",
    "rollup-copy-plugin": "^0.1.0",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-commonjs": "^9.1.4",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^1.0.1",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-vue": "^4.3.0",
    "sass-loader": "^7.0.3",
    "ts-jest": "^23.0.1",
    "typescript": "^2.9.2",
    "vue": "^2.5.16",
    "vue-class-component": "^6.0.0",
    "vue-property-decorator": "^7.0.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.3",
    "webpack-merge": "^4.1.3"
  },
  "peerDependencies": {
    "vue": "^2.5.16"
  }
}
